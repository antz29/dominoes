<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <sampleQuery>select * from {table} where url='your_url';</sampleQuery>
  </meta>
  <bindings>
    <select itemPath="" produces="JSON">
      <inputs>
        <key id='url' type='xs:string' paramType='path' required="true" batchable="true" maxBatchItems="10" />
      </inputs>
      <execute><![CDATA[
          var data = y.rest(url).accept('text/plain').get().response;
          response.object = {
          	url: url,
          	data: data
          };
      ]]></execute>
    </select>
  </bindings>
</table>